{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "max-line-length": {
            "options": [
                120
            ]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": false,
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                120
            ]
        }
    },
    "rulesDirectory": []
}